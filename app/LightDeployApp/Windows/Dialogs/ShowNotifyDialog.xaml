<Window x:Class="LightDeployApp.Windows.Dialogs.ShowNotifyDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:LightDeployApp.Windows.Dialogs"
        mc:Ignorable="d"
        Title="ShowNotifyDialog" Height="450" Width="800">
    <StackPanel>
        <Label Margin="10">发布说明:</Label>
        <TextBox MinLines="10"  AcceptsReturn="True" Margin="10" Name="NotifyContent">
            
        </TextBox>
        <Button Margin="10" Click="NotifyClick">通知</Button>
        <Button Margin="10" Click="CancelClick">不通知</Button>
    </StackPanel>
</Window>
